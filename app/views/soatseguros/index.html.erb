<p id="notice"><%= notice %></p>

<h1>Soatseguros</h1>

<table>
  <thead>
    <tr>
      <th>Placa</th>
      <th>Del</th>
      <th>Vehiculo</th>
      <th>Fecha</th>
      <th>Inicio</th>
      <th>Vigencia</th>
      <th>Fecha</th>
      <th>Fin</th>
      <th>Vigencia</th>
      <th>Numero</th>
      <th>Pasajeros</th>
      <th>Cilindraje</th>
      <th>Clase</th>
      <th>Toneladas</th>
      <th>Edad</th>
      <th>Precio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @soatseguros.each do |soatseguro| %>
      <tr>
        <td><%= soatseguro.Placa %></td>
        <td><%= soatseguro.del %></td>
        <td><%= soatseguro.vehiculo %></td>
        <td><%= soatseguro.Fecha %></td>
        <td><%= soatseguro.inicio %></td>
        <td><%= soatseguro.vigencia %></td>
        <td><%= soatseguro.Fecha %></td>
        <td><%= soatseguro.fin %></td>
        <td><%= soatseguro.vigencia %></td>
        <td><%= soatseguro.Numero %></td>
        <td><%= soatseguro.pasajeros %></td>
        <td><%= soatseguro.Cilindraje %></td>
        <td><%= soatseguro.Clase %></td>
        <td><%= soatseguro.Toneladas %></td>
        <td><%= soatseguro.Edad %></td>
        <td><%= soatseguro.Precio %></td>
        <td><%= link_to 'Show', soatseguro %></td>
        <td><%= link_to 'Edit', edit_soatseguro_path(soatseguro) %></td>
        <td><%= link_to 'Destroy', soatseguro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Soatseguro', new_soatseguro_path %>
